<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:orientation="vertical"
	android:fitsSystemWindows="true"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <android.support.v4.widget.DrawerLayout
	android:id="@id/drawer_layout"
	android:fitsSystemWindows="true"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
        <android.support.design.widget.AppBarLayout
	android:id="@id/appbar_toolbar"
	android:fitsSystemWindows="true"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">
            <android.support.v7.widget.Toolbar
	android:id="@id/toolbar"
	android:paddingTop="10.0dip"
	android:fitsSystemWindows="true"
	android:layout_width="fill_parent"
	android:layout_height="90.0dip"
	android:minHeight="?actionBarSize"
	android:layout_centerInParent="true" android:popupTheme="@style/ThemeOverlay.AppCompat.Light" xmlns:app="http://schemas.android.com/apk/res-auto">
                <RelativeLayout
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
                    <ImageView
	android:layout_gravity="center_horizontal"
	android:id="@id/tv_header_title"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:layout_marginBottom="5.0dip"
	android:src="@drawable/logo_home"
	android:layout_centerHorizontal="true" />
                </RelativeLayout>
            </android.support.v7.widget.Toolbar>
            <RelativeLayout
	android:id="@id/rl_account_info"
	android:background="@color/colorAccent"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">
                <TextView
	android:textSize="16.0dip"
	android:textColor="@color/white"
	android:id="@id/tv_account_info"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="12.0dip"
	android:layout_marginBottom="12.0dip"
	android:text="@string/movie_info"
	android:layout_centerHorizontal="true"
	android:fontFamily="@string/font_family_bold" />
            </RelativeLayout>
            <ScrollView
	android:layout_width="wrap_content"
	android:layout_height="fill_parent">
                <RelativeLayout
	android:layout_width="wrap_content"
	android:layout_height="fill_parent">
                    <RelativeLayout
	android:background="@color/light_gray"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
                        <RelativeLayout
	android:id="@id/rl_movie_image"
	android:layout_width="150.0dip"
	android:layout_height="200.0dip">
                            <ImageView
	android:id="@id/iv_movie_image"
	android:background="@drawable/noposter"
	android:layout_width="150.0dip"
	android:layout_height="200.0dip"
	android:layout_marginTop="10.0dip"
	android:scaleType="fitXY"
	android:layout_marginStart="20.0dip" />><ImageView
	android:id="@id/iv_favourite"
	android:background="@drawable/selector_favourite_vod"
	android:layout_width="40.0dip"
	android:layout_height="40.0dip"
	android:layout_marginTop="10.0dip"
	android:layout_marginRight="1.0dip"
	android:src="@drawable/favourite_yellow"
	android:layout_alignParentRight="true" />
                        </RelativeLayout>><ProgressBar
	android:gravity="center"
	android:id="@id/tv_detail_ProgressBar"
	android:layout_width="fill_parent"
	android:layout_height="30.0dip"
	android:layout_marginTop="63.0dip"
	android:layout_toRightOf="@id/rl_movie_image"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" style="?android:progressBarStyleSmall" />
                        <TextView
	android:textSize="15.0sp"
	android:textStyle="bold"
	android:gravity="center"
	android:id="@id/tv_movie_name"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_marginTop="20.0dip"
	android:layout_toRightOf="@id/rl_movie_image"
	android:fontFamily="@string/font_family_medium"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" />
                        <TextView
	android:textSize="12.0sp"
	android:textStyle="italic"
	android:gravity="center"
	android:id="@id/tv_movie_genere"
	android:visibility="gone"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_marginTop="10.0dip"
	android:layout_toRightOf="@id/rl_movie_image"
	android:layout_below="@id/tv_movie_name"
	android:fontFamily="@string/font_family_medium"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" />
                        <ScrollView
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/rl_movie_image"
	android:layout_below="@id/tv_movie_genere">
                            <RelativeLayout
	android:padding="8.0dip"
	android:layout_width="fill_parent"
	android:layout_height="150.0dip"
	android:layout_marginEnd="10.0dip">
                                <RelativeLayout
	android:gravity="center_horizontal"
	android:id="@id/rl_info"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_centerInParent="true">
                                    <TextView
	android:textStyle="bold"
	android:id="@id/tv_director"
	android:visibility="gone"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:text="@string/diected_by" />><TextView
	android:id="@id/tv_director_info"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:layout_toRightOf="@id/tv_director"
	android:layout_marginStart="30.0dip" />><TextView
	android:textStyle="bold"
	android:id="@id/tv_rating"
	android:visibility="gone"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:text="@string/rating"
	android:layout_below="@id/tv_director_info" />><TextView
	android:id="@id/tv_rating_label"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:layout_toRightOf="@id/tv_rating"
	android:layout_below="@id/tv_director_info"
	android:layout_marginStart="62.0dip" />><TextView
	android:textStyle="bold"
	android:id="@id/tv_release_date"
	android:visibility="gone"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:text="@string/release_date"
	android:layout_below="@id/tv_rating_label" />><TextView
	android:id="@id/tv_release_date_info"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:layout_toRightOf="@id/tv_release_date"
	android:layout_below="@id/tv_rating_label"
	android:layout_marginStart="20.0dip" />></RelativeLayout>
                            </RelativeLayout>></ScrollView>><TextView
	android:textSize="20.0sp"
	android:textColor="@color/black"
	android:gravity="center"
	android:id="@id/tv_play"
	android:background="@drawable/selector_play"
	android:focusable="true"
	android:focusableInTouchMode="true"
	android:nextFocusDown="@id/tv_add_to_fav"
	android:layout_width="fill_parent"
	android:layout_height="50.0dip"
	android:layout_marginTop="20.0dip"
	android:text="@string/nav_play"
	android:layout_below="@id/rl_movie_image"
	android:layout_centerHorizontal="true"
	android:fontFamily="@string/font_family_medium"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" />
                        <TextView
	android:textSize="20.0sp"
	android:textColor="@color/black"
	android:gravity="center"
	android:id="@id/tv_add_to_fav"
	android:background="@drawable/selector_play"
	android:focusable="true"
	android:nextFocusUp="@id/tv_play"
	android:layout_width="fill_parent"
	android:layout_height="50.0dip"
	android:layout_marginTop="20.0dip"
	android:text="@string/add_to_favourite"
	android:layout_below="@id/tv_play"
	android:layout_centerHorizontal="true"
	android:fontFamily="@string/font_family_medium"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" />
                        <TextView
	android:textSize="20.0sp"
	android:textColor="@color/black"
	android:gravity="center"
	android:id="@id/tv_detail_back_btn"
	android:background="@drawable/selector_play"
	android:focusable="true"
	android:nextFocusUp="@id/tv_add_to_fav"
	android:layout_width="fill_parent"
	android:layout_height="50.0dip"
	android:layout_marginTop="20.0dip"
	android:text="@string/back_button"
	android:layout_below="@id/tv_add_to_fav"
	android:layout_centerHorizontal="true"
	android:fontFamily="@string/font_family_medium"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip" />
                        <RelativeLayout
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="20.0dip"
	android:layout_below="@id/tv_detail_back_btn"
	android:layout_marginStart="20.0dip"
	android:layout_marginEnd="20.0dip">
                            <RelativeLayout
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">
                                <TextView
	android:id="@id/tv_cast"
	android:visibility="gone"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:text="@string/cast" />><TextView
	android:id="@id/tv_cast_info"
	android:visibility="gone"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="5.0dip"
	android:layout_marginBottom="20.0dip"
	android:layout_toRightOf="@id/tv_cast"
	android:layout_marginStart="88.0dip" />><TextView
	android:id="@id/tv_movie_info"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="10.0dip"
	android:layout_marginBottom="20.0dip"
	android:layout_below="@id/tv_cast" />></RelativeLayout>></RelativeLayout>></RelativeLayout>
                </RelativeLayout>></ScrollView>><include
	android:layout_width="fill_parent"
	android:layout_height="fill_parent" layout="@layout/app_bar_drawer" />
        </android.support.design.widget.AppBarLayout>
        <android.support.design.widget.NavigationView
	android:layout_gravity="start"
	android:id="@id/nav_view"
	android:fitsSystemWindows="true"
	android:layout_width="wrap_content"
	android:layout_height="fill_parent" app:headerLayout="@layout/nav_header_drawer" android:itemBackground="@layout/nav_view_item_background" app:menu="@menu/menu_dashboard" xmlns:app="http://schemas.android.com/apk/res-auto" />
    </android.support.v4.widget.DrawerLayout>
</LinearLayout>